print "Printing using a single loop";
print "==========";

var a = 0;
var temp;

for (var b = 1; a < 10000; b = temp + b) {
    print a;
    temp = a;
    a = b;
}

print "==========";
print "Printing using fib(...)";
print "==========";

fun fib(n) {
    if (n <= 1) return n;
    return fib(n - 2) + fib(n - 1);
}

for (var i = 0; i < 21; i = i + 1) {
    print fib(i);
}
